NEXT_PUBLIC_APP_NAME=Boilerplate
PORT=3000

# API key should be the same as in the backend app.
API_KEY=

NEXT_PUBLIC_BACKEND_URL=http://localhost:3001
NEXT_PUBLIC_FRONTEND_URL=http://localhost:3000
# Auth url should be the same as the frontend
NEXTAUTH_URL=$NEXT_PUBLIC_FRONTEND_URL
# This is a random auth secret. You can generate one using `openssl rand -base64 64` command.
NEXTAUTH_SECRET=

# Google Auth
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

# Microsoft Auth
AZURE_CLIENT_ID=
AZURE_CLIENT_SECRET=
AZURE_TENANT_ID=

# If not empty, NextJS will rewrite requests from this path
# to the NEXT_PUBLIC_BACKEND_URL
# (should be used in a dev mode only)
NEXT_PUBLIC_BACKEND_PROXY_PATH=/api

# URL used in next.config.js to set resources for image loading
CDN_BASE_URL=http://localhost:3000/api/uploads

#Google Recaptcha
NEXT_PUBLIC_RECAPTCHA_SITE_KEY=
RECAPTCHA_SECRET_KEY=